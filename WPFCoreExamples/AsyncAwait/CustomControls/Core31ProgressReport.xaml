<UserControl x:Class="WPFCoreExamples.AsyncAwait.CustomControls.Core31ProgressReport"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPFCoreExamples.AsyncAwait.CustomControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel>
            <TextBlock>Report Progress Example</TextBlock>
            <Button x:Name="ProgressBarButton" Click="ProgressBarButton_Click">Start</Button>
            <ProgressBar x:Name="ProgressBar"
                         Height="20"></ProgressBar>
        </StackPanel>
    </Grid>
</UserControl>
